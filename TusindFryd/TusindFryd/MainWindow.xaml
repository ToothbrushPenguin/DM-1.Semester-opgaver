<Window x:Class="TusindFryd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TusindFryd"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="300">
    <Grid>

        <ListBox x:Name="BlomsterSortList"
                 Width="220"
                 Height="150"
                 HorizontalAlignment="Center"
                 VerticalAlignment="Top"
                 Background="LightGray"
                 ></ListBox>
        <Button x:Name="Transporter"
                Width="150"
                Height="50"
                HorizontalAlignment="Center"
                VerticalAlignment="Top" Margin="0,167,0,0" Click="Transporter_Click" Content="OpretBlomsterSort"
                
                ></Button>

    </Grid>
</Window>
